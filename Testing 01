    {
      "cell_type": "code",
      "source": [
        "import json\n",
        "\n",
        "# Path to your JSON file\n",
        "file_path = r\"C:\\Users\\amirs\\Documents\\QADataset\"\n",
        "\n",
        "\n",
        "# Load JSON data\n",
        "with open(file_path, \"r\", encoding=\"utf-8\") as file:\n",
        "    data = json.load(file)\n",
        "\n",
        "# Function to categorize answers based on length\n",
        "def categorize_answer_length(answer_text):\n",
        "    word_count = len(answer_text.split())\n",
        "    if word_count <= 5:\n",
        "        return \"short\"\n",
        "    elif word_count <= 20:\n",
        "        return \"moderate\"\n",
        "    else:\n",
        "        return \"long\"\n",
        "\n",
        "# Add difficulty field to each answer\n",
        "for item in data:\n",
        "    for answer in item[\"answers\"]:\n",
        "        answer[\"difficulty\"] = categorize_answer_length(answer[\"text\"])\n",
        "\n",
        "# Save the updated dataset\n",
        "updated_file_path = r\"C:\\Users\\amirs\\Documents\\questionnaire_labeled.json\"\n",
        "with open(updated_file_path, \"w\", encoding=\"utf-8\") as file:\n",
        "    json.dump(data, file, indent=4)\n",
        "\n",
        "print(f\"Labeled dataset saved to {updated_file_path}\")\n"
      ],
