      "cell_type": "code",
      "source": [
        "\n",
        "\n",
        "# Read the text file\n",
        "with open(file_path, \"r\", encoding=\"utf-8\") as file:\n",
        "    lines = file.readlines()\n",
        "\n",
        "# Example assumption: Process lines into structured JSON format\n",
        "# Update this logic based on how your Q&A data is structured in the file\n",
        "for i in range(0, len(lines), 3):  # Assuming every 3 lines form Q, C, A\n",
        "    try:\n",
        "        question = lines[i].strip()  # Question on line 1\n",
        "        context = lines[i + 1].strip()  # Context on line 2\n",
        "        answer = lines[i + 2].strip()  # Answer on line 3\n",
        "\n",
        "        qa_entry = {\n",
        "            \"question\": question,\n",
        "            \"context\": context,\n",
        "            \"answers\": [{\"id\": \"1\", \"text\": answer}]\n",
        "        }\n",
        "        qa_data.append(qa_entry)\n",
        "    except IndexError:\n",
        "        print(\"Skipping incomplete Q&A set at the end of file.\")\n",
        "        break\n",
        "\n",
        "# Output JSON file path\n",
        "output_path = r\"C:\\Users\\<YourUsername>\\Documents\\Q&A-Dataset.json\"  # Replace <YourUsername> with your actual username\n",
        "\n",
        "# Save the structured data into a JSON file\n",
        "with open(output_path, \"w\", encoding=\"utf-8\") as json_file:\n",
        "    json.dump(qa_data, json_file, indent=4, ensure_ascii=False)\n",
        "\n",
        "print(f\"Q&A dataset has been saved to: {output_path}\")\n"
      ],
